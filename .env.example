# TimeVault Environment Configuration

# Development
VITE_APP_NAME=TimeVault
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Convert crypto to gold, silver & time

# API Configuration
VITE_COINGECKO_API_URL=https://api.coingecko.com/api/v3
VITE_METALS_API_URL=https://api.metals.live/v1
VITE_API_TIMEOUT=10000

# Thirdweb Configuration (XRPL)
VITE_THIRDWEB_CLIENT_ID=your_thirdweb_client_id_here
VITE_XRPL_NETWORK=testnet

# Feature Flags
VITE_ENABLE_PREMIUM_FEATURES=true
VITE_ENABLE_AI_INSIGHTS=true
VITE_ENABLE_WALLET_CONNECT=true

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Social Links
VITE_TWITTER_URL=https://twitter.com/timevault
VITE_DISCORD_URL=https://discord.gg/timevault
VITE_GITHUB_URL=https://github.com/timevault

# Contact
VITE_SUPPORT_EMAIL=support@timevault.app
VITE_CONTACT_EMAIL=contact@timevault.app
