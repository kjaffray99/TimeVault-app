# TimeVault Production Environment Variables - Revenue Critical
# These ensure the app loads even if vars are missing

# Core Application Settings
VITE_APP_NAME=TimeVault
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Performance and Caching (Revenue Critical)
VITE_CACHE_ENABLED=true
VITE_CACHE_DURATION=300000
VITE_RATE_LIMIT_MAX=50
VITE_RATE_LIMIT_WINDOW=60000

# Security Settings (Compliance Required)
VITE_ENHANCED_SECURITY=true
VITE_SECURITY_STRICT_MODE=true
VITE_CSP_ENABLED=true
VITE_XSS_PROTECTION=true

# Analytics and Tracking (Revenue Optimization)
VITE_ANALYTICS_ENABLED=true
VITE_ADVANCED_METRICS=true
VITE_CONVERSION_TRACKING=true
VITE_GA_MEASUREMENT_ID=GA_MEASUREMENT_ID

# API Configuration (External Services)
VITE_COINGECKO_API_KEY=demo-key
VITE_METALS_API_KEY=demo-key
VITE_API_TIMEOUT=10000
VITE_API_RETRY_COUNT=3

# Feature Flags (Phase Control)
VITE_ENABLE_PREMIUM=true
VITE_ENABLE_SOCIAL_SHARING=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_GAMIFICATION=true
VITE_ENABLE_NFT_FEATURES=true

# Revenue Features
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_demo
VITE_PREMIUM_TIERS=99,199,499
VITE_CONVERSION_GOAL_VALUE=200

# Compliance and Legal
VITE_SHOW_DISCLAIMERS=true
VITE_GDPR_ENABLED=true
VITE_COMPLIANCE_REQUIRED=true

# Development and Debugging
VITE_DEBUG_MODE=false
VITE_ERROR_REPORTING=true
VITE_PERFORMANCE_MONITORING=true
