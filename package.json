{
  "name": "timevault-mvp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "TimeVault - Digital asset to precious metals and time converter",
  "keywords": [
    "crypto",
    "precious metals",
    "calculator",
    "fintech",
    "react"
  ],
  "author": "TimeVault Team",
  "license": "MIT",
  "homepage": "https://timevaultai.com",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:secure": "npm run security:validate && npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "security:validate": "node scripts/validate-security.js",
    "security:audit": "npm audit --audit-level=high",
    "backtest": "node scripts/backtest-deployment.mjs",
    "backtest:full": "npm run backtest && npm run security:audit",
    "deploy:vercel": "npm run build:secure && vercel --prod",
    "deploy:netlify": "npm run build:secure && netlify deploy --prod --dir=dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.8.0",
    "@stripe/stripe-js": "^7.6.1",
    "@tanstack/react-query": "^5.83.0",
    "@thirdweb-dev/react": "^4.9.4",
    "@thirdweb-dev/sdk": "^4.0.99",
    "@types/dompurify": "^3.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.29",
    "axios": "^1.6.0",
    "dompurify": "^3.2.6",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
