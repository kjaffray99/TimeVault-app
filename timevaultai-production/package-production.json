{
    "name": "timevault-ai-production",
    "version": "2.0.0",
    "description": "TimeVault AI - Professional Crypto Calculator with Live Revenue Generation",
    "scripts": {
        "dev": "next dev -p 3003",
        "build": "next build",
        "build:production": "NODE_ENV=production next build",
        "start": "next start -p 3003",
        "start:production": "NODE_ENV=production next start -p 3003",
        "lint": "next lint",
        "type-check": "tsc --noEmit",
        "deploy": "npm run build:production && npm run start:production",
        "deploy:vercel": "vercel --prod",
        "deploy:timevaultai": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-production.ps1",
        "test": "jest",
        "test:e2e": "playwright test",
        "validate": "npm run type-check && npm run lint && npm run test",
        "stripe:setup": "stripe listen --forward-to localhost:3003/api/webhooks/stripe",
        "analytics:setup": "node scripts/setup-analytics.js",
        "revenue:dashboard": "node scripts/revenue-dashboard.js"
    },
    "dependencies": {
        "next": "15.4.5",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "stripe": "^16.12.0",
        "@types/node": "22.10.2",
        "@types/react": "19.0.2",
        "@types/react-dom": "19.0.2",
        "typescript": "5.7.3",
        "tailwindcss": "^3.4.0",
        "axios": "^1.7.0",
        "recharts": "^2.12.0",
        "@thirdweb-dev/react": "^4.0.0",
        "@thirdweb-dev/sdk": "^4.0.0",
        "framer-motion": "^11.0.0",
        "lucide-react": "^0.400.0",
        "date-fns": "^3.0.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "jest": "^29.7.0",
        "playwright": "^1.40.0",
        "eslint": "^8.57.0",
        "eslint-config-next": "15.4.5",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.32"
    },
    "keywords": [
        "cryptocurrency",
        "precious metals",
        "calculator",
        "bitcoin",
        "ethereum",
        "gold",
        "silver",
        "stripe",
        "payments",
        "revenue",
        "timevault",
        "nextjs",
        "typescript"
    ],
    "author": "TimeVault AI Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/kjaffray99/TimeVault-app.git"
    },
    "homepage": "https://timevaultai.com",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "private": false
}