{
  "name": "timevaultai-production",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3003",
    "dev:port": "next dev --turbopack -p 3003",
    "build": "next build",
    "start": "next start -p 3003",
    "production": "npm run build && npm run start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true npm run build",
    "validate": "node scripts/validate-production.js",
    "deploy": "npm run validate && npm run build && npm run validate",
    "test:production": "cross-env TEST_SERVER=true npm run validate",
    "comprehensive-test": "node scripts/comprehensive-validation.js",
    "enhanced-deploy": "node scripts/enhanced-deploy.js",
    "test:live": "npm run comprehensive-test && npm run enhanced-deploy",
    "launch": "npm run enhanced-deploy",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "deploy:vercel": "npm run build:production && vercel --prod"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.7.0",
    "@tanstack/react-query": "^5.83.0",
    "@thirdweb-dev/react": "^3.10.3",
    "@thirdweb-dev/sdk": "^4.0.60",
    "critters": "^0.0.23",
    "framer-motion": "^12.23.11",
    "geist": "^1.4.2",
    "lucide-react": "^0.534.0",
    "next": "15.4.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^3.1.0",
    "stripe": "^18.3.0",
    "thirdweb": "^5.105.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.5",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
